{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "full-stack-professional",
      "image": "togrulmamishov/spring-boot-fullstack:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "aws"
        }
      ]
    }
  ]
}