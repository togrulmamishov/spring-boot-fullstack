{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "customer-api-react",
      "image": "togrulmamishov/customer-api-react:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    },
    {
      "name": "full-stack-professional",
      "image": "togrulmamishov/spring-boot-fullstack:12.08.2023.16.56.26",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "aws"
        }
      ]
    }
  ]
}